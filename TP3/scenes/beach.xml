<lsf>
  <initials>
    <reference length="0"/>
    <root id="root"/>
  </initials>

  <views default="Default">
    <perspective id="Default" near="0.1" far="500" angle="60">
      <from x="5" y="5" z="0"/>
      <to x="0" y="0" z="0"/>
    </perspective>
  </views>

  <illumination>
    <ambient r="0.5" g="0.5" b="0.5" a="1.0"/>
    <background r="1.0" g="0.90" b="1.0" a="1.0"/>
  </illumination>

  <lights>
    <light id="default">
      <!-- light identifier -->
      <enable value="1"/>
      <!-- enable/disable -->
      <position x="0" y="1" z="0" w="0"/>
      <!-- light position; w=1: point light; w=0: directional light -->
      <ambient r="1" g="1" b="1" a="1.0"/>
      <!-- ambient component -->
      <diffuse r="1" g="1" b="1" a="1.0"/>
      <!-- diffuse component -->
      <specular r="0.5" g="0.5" b="0.5" a="1.0"/>
      <!-- specular component -->
    </light>
  </lights>

